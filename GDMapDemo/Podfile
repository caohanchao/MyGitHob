    pod 'AMapNavi' #导航SDK
    pod 'AMapLocation'
    
  target 'GDMapDemoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GDMapDemoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
